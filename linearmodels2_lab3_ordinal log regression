*call summary stats of response variable;
proc freq data=cancer;
	Table RESP;
run;

proc print data=cancer; run;


*a proportional odds model with ascending formulation; 
proc logistic data=cancer;
	class therapy(ref='Seq') gender(ref='F') /param=reference;
	model resp = age gender therapy;
run;

*a proportional odds model with descending formulation; 
proc logistic data=cancer;
	class therapy(ref='Seq') gender(ref='F') /param=reference;
	model resp(descending) = age gender therapy/scale=none aggregate;
run;

proc freq data=cancer; 
Table agecat * gender * therapy * resp /nocol norow nopercent;
run;



